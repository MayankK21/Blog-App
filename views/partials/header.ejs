<html><head>	<title>Awesome Portfolio Site</title>	<link rel="stylesheet" href="https://use.fontawesome.com/releases/v5.5.0/css/all.css">	<link rel="stylesheet" type="text/css" href="/bootstrap.css">	<link rel="stylesheet" type="text/css" href="/app.css">		<script src="https://code.jquery.com/jquery-3.3.1.slim.min.js" integrity="sha384-q8i/X+965DzO0rT7abK41JStQIAqVgRVzpbzo5smXKp4YfRvH+8abtTE1Pi6jizo" crossorigin="anonymous">	</script>	<script src="https://cdnjs.cloudflare.com/ajax/libs/popper.js/1.14.7/umd/popper.min.js" integrity="sha384-UO2eT0CpHqdSJQ6hJty5KVphtPhzWj9WO1clHTMGa3JDZwrnQq4sF86dIHNDz0W1" crossorigin="anonymous">	</script>	<script src="https://stackpath.bootstrapcdn.com/bootstrap/4.3.1/js/bootstrap.min.js" integrity="sha384-JjSmVgyd0p3pXB1rRibZUAYoIIy6OrQ6VrjIEaFf/nJGzIxFDsf4x0xIM+B07jRM" crossorigin="anonymous">	</script>	</head><body><nav class="nav navbar-dark bg-dark ">	<div class="container">		<a class="navbar-brand" href="/">Blog Site</a>		<a href="/blogs" class="btn">Home</a>		<a href="/blogs/new" class="btn">New Blog</a>		<% if(currentUser){ %>			<a href="/logout" class="btn float-right">Logout</a>			<a href="/show" class="btn">Show Database</a>			<a style="color: white;" class="btn float-right navbar-brand" disabled> Logged in as: <%= currentUser.firstName %>				<img src="<%= currentUser.image %>" width="30" height="30" class="d-inline-block img-fluid rounded" alt="">			</a>		<% } else { %>			<a href="/register" class="btn float-right">Sign Up <i class="fas fa-user-plus"></i></a>			<a href="/login" class="btn float-right">Login <i class="fas fa-user"></i></a>			<% } %>					</div></nav><div style="margin-top: 10px;" class="container">	<% if(error && error.length > 0){ %>		<div class="alert alert-danger alert-dismissible fade show" role="alert">			<strong><%= error %></strong>			<button type="button" class="close" data-dismiss="alert" aria-label="Close">		    	<span aria-hidden="true">&times;</span>		  	</button>		</div>	<% } %>	<% if(success && success.length > 0){ %>		<div class="alert alert-success alert-dismissible fade show" role="alert">			<strong><%= success %></strong>			<button type="button" class="close" data-dismiss="alert" aria-label="Close">		    	<span aria-hidden="true">&times;</span>		  	</button>		</div>	<% } %>	</div>